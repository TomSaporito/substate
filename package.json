{
  "name": "substate",
  "version": "0.0.0-alpha.1",
  "description": "Simple State Management with Optional Deep Cloning",
  "main": "dist/index.js",
  "scripts": {
    "deploy": "cd browser-test && npm install && npm run build && cd .. && node clean && gh-pages -d browser-test/dist",
    "start": "node clean && npm run build && cd browser-test && npm install && npm start",
    "test": "npm run build && jest",
    "build": "npm run quick:build && npm pack",
    "quick:build": "rollup -c rollup.config.js",
    "publish": "npm run test && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TomSaporito/substate.git"
  },
  "keywords": [
    "state",
    "flux",
    "pubsub",
    "immutability",
    "modules",
    "state management"
  ],
  "author": "Tamb (Tom Saporito) <t.saporito@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.6.0",
    "@types/jest": "^25.2.3",
    "babel-loader": "^8.0.6",
    "gh-pages": "^2.1.1",
    "jest": "^26.0.1",
    "rimraf": "^3.0.0",
    "rollup": "^1.21.4",
    "rollup-plugin-terser": "^6.1.0",
    "ts-jest": "^26.0.0"
  },
  "dependencies": {
    "deep-clone-simple": "^1.1.1",
    "object-bystring": "^4.1.0"
  },
  "files": [
    "LICENSE",
    "README.md",
    "dist"
  ]
}
